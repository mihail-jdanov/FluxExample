import Foundation
import ComposableArchitecture

@Reducer
struct {{ module_info.name }}Reducer {

    typealias State = {{ module_info.name }}State
    typealias Action = {{ module_info.name }}Action

    var body: some ReducerOf<Self> {
        BindingReducer()
        Reduce { state, action in
            switch action {
            case .binding:
                return .none
            case .someAction:
                state.message = "Changed state"
                return .none
            }
        }
    }

}